{
    "name": "@actlib/source",
    "version": "1.0.0",
    "license": "MIT",
    "private": true,
    "scripts": {
        "preinstall": "npx only-allow yarn",
        "upgrade-self": "yarn set version latest && yarn",
        "update-nx": "nx migrate latest && nx migrate --run-migrations --if-exists",
        "prettier": "prettier --write .",
        "lint:": "yarn nx affected -t lint",
        "lint:fix": "eslint . --fix",
        "prepare": "husky",
        "cmt": "npx cz",
        "cq": "yarn prettier && yarn lint",
        "release": "yarn nx run-many -t build && tsx tools/release.ts"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-nx-scopes": "^19.8.0",
        "@eslint/js": "^9.21.0",
        "@nx/eslint": "20.5.0",
        "@nx/eslint-plugin": "20.5.0",
        "@nx/js": "20.5.0",
        "@swc-node/register": "1.10.9",
        "@swc/core": "1.11.8",
        "@swc/helpers": "0.5.15",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/fs-extra": "^11",
        "@types/node": "22.13.10",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-check-file": "^3.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-promise": "^7.2.1",
        "fs-extra": "^11.3.0",
        "husky": "^9.1.7",
        "jsonc-eslint-parser": "^2.4.0",
        "lint-staged": "^15.4.3",
        "nx": "20.5.0",
        "prettier": "^3.5.3",
        "prettier-plugin-sort-json": "^4.1.1",
        "tslib": "^2.8.1",
        "tsx": "^4.19.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.0"
    },
    "workspaces": [
        "packages/*"
    ],
    "engines": {
        "node": ">=22",
        "yarn": ">=4.7.0"
    },
    "packageManager": "yarn@4.7.0"
}
